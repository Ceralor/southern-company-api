{
  "name": "southern-company-api",
  "version": "0.2.0",
  "description": "A Library for pulling power usage data from Southern Company or Southern Company subcompanies",
  "main": "main.js",
  "scripts": {
    "test": "node test.js"
  },
  "author": "John Andrew Tyler Pearson",
  "license": "MIT",
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "info",
            "warn",
            "error"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "axios": "^0.16.1",
    "moment": "^2.17.1"
  }
}
